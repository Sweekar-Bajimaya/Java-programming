import java.util.*;

public class Listing {
	public static void main(String[] args) {
		List fruits = new ArrayList();
		fruits.add("apple");
		fruits.add("banana");
		fruits.add("pear");
		fruits.add("cherry");
		System.out.println(fruits);
		
		fruits.set(0, "Grapefruit");
		fruits.set(2, "Date");
		fruits.add(5, "Orange");
		
		System.out.println(fruits);
				
	}
}
